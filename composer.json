{
    "name": "pyaesoneaung/laravel-myanmar-tools",
    "description": "A package for Myanmar tools which extend Laravelâ€™s core.",
    "keywords": [
        "laravel-myanmar-tools",
        "php",
        "laravel",
        "myanmar",
        "myanmar-phone-number",
        "myanmar-zawgyi-to-unicode-converter",
        "myanmar-tools",
        "myanmar-unicode",
        "laravel-validation-rules"
    ],
    "type": "library",
    "homepage": "https://github.com/PyaeSoneAungRgn/laravel-myanmar-tools",
    "license": "MIT",
    "authors": [
        {
            "name": "Pyae Sone Aung",
            "email": "pyaesoneaungrgn@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0",
        "cyberwings/myanmar-phone-number-php": "^0.0.3",
        "googlei18n/myanmar-tools": "^1.2",
        "rabbit-converter/rabbit-php": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "pestphp/pest": "^1.22"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "PyaeSoneAung\\LaravelMyanmarTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PyaeSoneAung\\LaravelMyanmarTools\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PyaeSoneAung\\LaravelMyanmarTools\\LaravelMyanmarToolsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
