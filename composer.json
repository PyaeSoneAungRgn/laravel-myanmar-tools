{
    "name": "pyaesoneaung/laravel-myanmar-tools",
    "description": "A package for Myanmar tools which extend Laravelâ€™s core.",
    "keywords": [
        "laravel-myanmar-tools",
        "php",
        "laravel",
        "myanmar",
        "myanmar-phone-number",
        "myanmar-zawgyi-to-unicode-converter",
        "myanmar-tools",
        "myanmar-unicode",
        "laravel-validation-rules"
    ],
    "type": "library",
    "homepage": "https://github.com/PyaeSoneAungRgn/laravel-myanmar-tools",
    "license": "MIT",
    "authors": [
        {
            "name": "Pyae Sone Aung",
            "email": "pyaesoneaungrgn@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0|^10.0",
        "laravel-myanmar-tools/phone-number": "^1.0.1",
        "laravel-myanmar-tools/font": "^1.0",
        "laravel-myanmar-tools/nrc": "^1.0.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "pestphp/pest": "^1.22",
        "laravel/pint": "^1.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "PyaeSoneAung\\LaravelMyanmarTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PyaeSoneAung\\LaravelMyanmarTools\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PyaeSoneAung\\LaravelMyanmarTools\\LaravelMyanmarToolsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
